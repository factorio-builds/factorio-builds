{
  "name": "with-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "ts-node --project tsconfig.server.json server/index.ts",
    "format": "prettier --write {components,design,hooks,icons,pages,redux,styles,types,utils}/{**,**/**}/*.{js,ts,tsx}",
    "local": "DOTENV_CONFIG_PATH=./.env ts-node --project ./tsconfig.yarn.json -r dotenv/config",
    "start": "next start",
    "type-check": "tsc",
    "typeorm": "yarn local ./node_modules/typeorm/cli.js",
    "db:seed:config": "yarn local ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed:run": "yarn local ./node_modules/typeorm-seeding/dist/cli.js seed",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "formik": "^2.2.1",
    "next": "latest",
    "pako": "^1.0.11",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "polished": "^4.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.1",
    "react-markdown": "^5.0.2",
    "react-redux": "^7.2.1",
    "react-select": "^3.1.0",
    "react-uid": "^2.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "reflect-metadata": "^0.1.13",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "text-encoding": "^0.7.0",
    "typeorm": "^0.2.28",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.8",
    "@types/formidable": "^1.0.31",
    "@types/node": "^12.12.21",
    "@types/pako": "^1.0.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-select": "^3.0.22",
    "@types/styled-components": "^5.1.4",
    "@types/text-encoding": "^0.0.35",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.1.2",
    "request": "^2.88.2",
    "ts-node": "^9.0.0",
    "typescript": "4.0"
  }
}
